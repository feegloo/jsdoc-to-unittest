{
  "name": "jsdoc-test-generator",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --cache {src,tests}/**/*.js",
    "build": "babel-node index.js",
    "config": "babel-node index.js --config",
    "clean": "bin/clean"
  },
  "keywords": [],
  "author": "Jakub Ro≈ºek",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-import-resolver-jest": "^1.0.1",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "acorn": "^5.1.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "doctrine": "^2.0.0",
    "escodegen": "^1.9.0",
    "prettier-eslint": "^8.2.0",
    "yargs": "^9.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/fs.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverage": true,
    "cacheDirectory": ".cache/",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/__mocks__/",
      "<rootDir>/*.js",
      "trash-*.test.js"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1.js",
      "mocks/(.*)": "<rootDir>/__mocks__/$1.js",
      "jest-helpers/(.*)": "<rootDir>/jest-helpers/$1.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  }
}
