{
  "name": "jsdoc-test-generator",
  "version": "1.0.0",
  "description": "Generate jest/karma compliant test suite from any valid JS source containing jsdoc annotations.",
  "main": "index.js",
  "bin": {
    "docs-to-tests": "bin/generate"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --cache {src,tests}/**/*.js",
    "install": "NODE_ENV=production rollup -c",
    "build": "bin/build-markdown",
    "clean": "bin/clean"
  },
  "keywords": [],
  "author": "Jakub Ro≈ºek",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-import-resolver-jest": "^1.0.1",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^21.2.1",
    "markdown-include": "^0.4.3"
  },
  "dependencies": {
    "acorn": "^5.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-implicit-return": "^1.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "doctrine": "^2.0.0",
    "escodegen": "^1.9.0",
    "prettier-eslint": "^8.2.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "yargs": "^9.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "__mocks__/**/*.js",
      "!src/fs.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverage": true,
    "cacheDirectory": ".cache/",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "<rootDir>/*.js",
      "trash-*.test.js"
    ],
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1.js",
      "mocks/(.*)": "<rootDir>/__mocks__/$1.js",
      "jest-helpers/(.*)": "<rootDir>/jest-helpers/$1.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  }
}
